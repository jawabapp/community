{
    "name": "jawabapp/community",
    "description": "jawab community",
    "keywords": [
        "jawabapp",
        "community"
    ],
    "homepage": "https://github.com/jawabapp/community",
    "license": "MIT",
    "type": "library",
    "authors": [{
        "name": "Ibraheem Qanah",
        "email": "i.qanah@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "intervention/image": "^2.6",
        "webcraft/laravel-random": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Jawabapp\\Community\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jawabapp\\Community\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jawabapp\\Community\\CommunityServiceProvider"
            ],
            "aliases": {
                "Community": "Jawabapp\\Community\\CommunityFacade"
            }
        }
    }
}
